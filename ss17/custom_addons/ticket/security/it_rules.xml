<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="rule_it_reporter_own" model="ir.rule">
            <field name="name">Reporter: own tickets only</field>
            <field name="model_id" ref="model_it_ticket" />
            <field name="domain_force">[('create_uid','=',user.id)]</field>
            <field name="groups" eval="[(4, ref('ticket.group_it_reporter'))]" />
        </record>

        <record id="rule_it_technician_all" model="ir.rule">
            <field name="name">Technician: all tickets</field>
            <field name="model_id" ref="model_it_ticket" />
            <field name="domain_force">[(1,'=',1)]</field>
            <field name="groups" eval="[(4, ref('ticket.group_it_technician'))]" />
        </record>

        <record id="rule_it_manager_all" model="ir.rule">
            <field name="name">Manager: all tickets</field>
            <field name="model_id" ref="model_it_ticket" />
            <field name="domain_force">[(1,'=',1)]</field>
            <field name="groups" eval="[(4, ref('ticket.group_it_manager'))]" />
        </record>
    </data>
</odoo>